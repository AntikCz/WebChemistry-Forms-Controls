<form action="" method="post" enctype="multipart/form-data" id="frm-form">

<table>
<tr>
	<th><label for="frm-form-text"></label></th>

	<td><input type="text" name="text" id="frm-form-text" data-live-validation="form-text" class="text"></td>
</tr>

<tr>
	<th><label for="frm-form-select"></label></th>

	<td><select name="select" id="frm-form-select" data-live-validation="form-select"><option value="items">items</option></select></td>
</tr>

<tr>
	<th></th>

	<td><label for="frm-form-checkbox"><input type="checkbox" name="checkbox" id="frm-form-checkbox"></label></td>
</tr>

<tr>
	<th><label></label></th>

	<td><label><input type="checkbox" name="checkboxList[]" value="items">items</label></td>
</tr>

<tr>
	<th><label for="frm-form-editor"></label></th>

	<td><textarea name="editor" id="frm-form-editor" data-live-validation="form-editor" class="editor-input ckeditor" data-novalidate=""></textarea></td>
</tr>

<tr>
	<th><label for="frm-form-mask"></label></th>

	<td><input type="text" name="mask" id="frm-form-mask" data-live-validation="form-mask" class="text"></td>
</tr>

<tr>
	<th><label for="frm-form-multiselect"></label></th>

	<td><select name="multiselect[]" id="frm-form-multiselect" multiple data-live-validation="form-multiselect"><option value="items">items</option></select></td>
</tr>

<tr>
	<th><label for="frm-form-password"></label></th>

	<td><input type="password" name="password" id="frm-form-password" data-live-validation="form-password" class="text"></td>
</tr>

<tr>
	<th><label for="frm-form-upload"></label></th>

	<td><input type="file" name="upload" id="frm-form-upload" data-live-validation="form-upload" class="text"></td>
</tr>

<tr>
	<th><label></label></th>

	<td><label><input type="radio" name="radioList" value="items">items</label></td>
</tr>

<tr>
	<th><label for="frm-form-textArea"></label></th>

	<td><textarea name="textArea" id="frm-form-textArea" data-live-validation="form-textArea"></textarea></td>
</tr>

<tr>
	<th><label for="frm-form-tags"></label></th>

	<td><input type="text" name="tags" placeholder="For the distribution of words please use comma." id="frm-form-tags" value="" data-live-validation="form-tags" class="tag-input text"></td>
</tr>
</table>

<div><input type="hidden" name="do" value="form-submit"></div>
</form>
